1 rem aipainter 2.05 cls10 graphics window 200,100,200,20015 call "wintitle","AI Painter 2.0"20 gosub 500 : rem random point generator25 gosub 600 : rem point plotter30 gosub 700 : rem point connector100 end : 'false500 rem _____________________________ point generator505 n = 200507 dim xc(n) : dim yc(n)510 for l = 1 to n515 xc(l) = rnd(200) : yc(l) = rnd(200)520 next l522 print "Print Coordinate Pairs? (y/n) "; : get pcp$523 if lcase$(pcp$) <> "y" then print : goto 545525 print "Coordinate Values:" : print : print "x";tab (10);"y"526 print530 for l = 1 to n535 print xc(l);tab (10);yc(l)540 next l545 print : print n;"pairs."550 return600 rem ___________________________ point plotter605 for l = 1 to n610 rem clr=rnd(100):clg = rnd(100):clb = rnd(100):rem old point color setters615 s = rnd(10) : rem sets size of point617 graphics color rnd(100),rnd(100),rnd(100)618 f = rnd(150) : if f < 50 then goto 625620 moveto xc(l),yc(l) : graphics circle s622 goto 630625 graphics filloval xc(l)-(s/2),yc(l)-(s/2),xc(l)+(s/2),yc(l)+(s/2)630 next l650 return700 rem ___________________________ point connector705 for l = 1 to n706 os = 2000707 for l2 = 1 to n710 graphics color rnd(100),rnd(100),rnd(100)715 moveto xc(l),yc(l)720 lineto xc(l2),yc(l2)722 rem goto 725723 sound os,0.02,20724 os = os-3725 next l2730 next l750 return